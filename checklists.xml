<checklists><name>Zhongtai Virtual CL650 Procedures</name><version>Jan 26 2025</version><groups><group name="NORMAL CHECKLIST" type="normal"><checklist name="Preflight checklist CR[F-]"><item name="Captain calls for the checklist after all preflight" color="magenta" /><item name="items and briefings are completed, with performance," color="magenta" /><item name="fuel, and clearance ready." color="magenta" /><item name=" 1. Flight Crew Briefing" action="Complete [C]" /><item name=" 2. ACARS/CPDLC Login" action="checked [F,C]" /><item name=" 3. Oxygen" action="checked [All]" /><item name=" 4. Pressurization" action="Set [C]" /><item name=" 5. ALTIMETER" action="_ inHg/hPa SET [F,C]" /><item name=" 6. flight Instruments" action="checked [F,C]" /><item name=" 7. Parking Brake" action="Set [C]" /><item name=" 8. Thrust levers" action="shutoff [C]" /></checklist><checklist name="Before start checklist CR[F-C]"><item name="Captain calls for the checklist after ALL doors" color="magenta" /><item name="closed and external support has been removed." color="magenta" /><item name=" 1. Passenger Sign" action="On/Auto [C]" /><item name=" 2. Fuel" action="Planned _, _ on board [C]" /><item name=" 3. Fuel Boost Pumps" action="on [C]" /><item name=" 4. Hyd Pumps" action="Checked / Auto / 3A On [C]" /><item name=" 5. Doors" action="Closed [C]" /></checklist><checklist name="After start Checklist CR[F-C]"><item name="Captain calls for the checklist after AFTER START" color="magenta" /><item name="flow has been completed." color="magenta" /><item name=" 1. GEN 1 &amp; 2" action="On [C]" /><item name=" 2. Flight Controls" action="Checked [C]" /><item name=" 3. ATS" action="N1-TO [C]" /><item name=" 4. ATC/TCAS" action="TA/RA [C]" /></checklist><checklist name="Before takeoff checklist CR[F-C]"><item name="Captain calls for the checklist as All performance" color="magenta" /><item name="and Flight guidance has been setup on FMC/FCP." color="magenta" /><item name=" 1. Continuous Ignition" action="armed" /><item name=" 2. Flaps" action="20" /><item name=" 3. FMC" action="_ (RWY, SID &amp; Trans), Set [F, C]" /><item name=" 4. EFIS/ATS/EICAS" action="Set [F, C]" /></checklist><checklist name="After Takeoff Checklist CR[PM-PM]"><item name="Pilot Flying calls for the checklist after flaps" color="magenta" /><item name="retract command." color="magenta" /><item name=" 1. Landing Gear" action="Up" /><item name=" 2. Flaps" action="Up" /><item name=" 3. Thrust Reversers" action="Off" /><item name=" 4. continuous ignition" action="Off" /><item name=" 5. Packs Transition" action="Completed" /><item name=" 6. N1 CLB" action="Set" /><item name=" 7. APU GEN" action="Off" /><item name=" 8. APU" action="Shutdown" /><item name=" 9. CAS" action="Clear" /></checklist><checklist name="&gt;&gt;Immediate return checklist&lt;&lt; RD"><item name=" 1. PRESSURIZATION" action="SET" /><item name=" 2. ALTIMETERS" action="SET / CROSS-checked" /><item name=" 3. THRUST REVERSERS" action="ARMED" /><item name=" 4. LANDING DATA" action="SET" /><item name=" 5. APPROACH BRIEFING" action="COMPLETE" /><item name=" 6. FUEL QUANTITY / BALANCE" action="COMPLETE" /><item name=" 7. CAS" action="checked" /><item name="Go to BEFORE LANDING CHECKLIST" color="magenta" /></checklist><checklist name="Descend checklist RD[PM]"><item name="Pilot Flying calls for the checklist when reaching 20,000ft." color="magenta" /><item name="APU in-flight start is guaranteed and authorized only under 20,000ft." color="magenta" /><item name=" 1. N1 TGT" action="TO" /><item name=" 2. APU" action="Start" /><item name=" 3. APU GEN" action="on" /><item name=" 4. External Lights" action="as Required" /><item name=" 5. Packs Transition" action="Completed" /><item name="Bleed air extraction from the APU is not permitted" color="magenta" /><item name="above 15,000 feet." color="magenta" /><item name=" 6. Passenger Signs" action="On" /><item name=" 7. Thrust reversers" action="armed" /><item name=" 8. CAS" action="Checked" /></checklist><checklist name="before landing checklist RD[PM]"><item name="Pilot Flying calls for the checklist after gear down" color="magenta" /><item name="and final flaps have been selected." color="magenta" /><item name=" 1. Continuous Ignition" action="On" /><item name=" 2. Gear" action="Down" /><item name=" 3. Nose Steer" action="Armed" /><item name=" 4. A/SKID" action="Armed/TEST" /><item name=" 5. Spoilers" action="Stowed/Auto" /><item name=" 6. Flaps" action="Set" /><item name=" 7. CAS" action="checked" /><item name="If using ATS, place physical throttle at IDLE" color="magenta" /><item name="position in case of inadvertent thrust lever" color="magenta" /><item name="position out-of-sync during go-around." color="magenta" /></checklist><checklist name="shutdown checklist CR"><item name=" 1. TAXI LIGHTS" action="OFF" /><item name=" 2. PARKING BRAKE" action="SET" /><item name=" 3. REVERSE THRUST" action="STOWED" /><item name=" 4. GEN 1 &amp; GEN 2" action="OFF" /><item name=" 5. ENGINE 2" action="SHUTDOWN" /><item name=" 6. FUEL BOOST PUMPS" action="OFF" /><item name=" 7. &gt;*&lt; FUEL SYSTEM" action="checked" /><item name=" 8. ENGINE 1" action="SHUTDOWN" /><item name="VERIFY ITT BELOW 350 DEGREES" color="magenta" /><item name=" 9. ANTI-ICE" action="OFF" /><item name="10. 14TH STAGE" action="OFF" /><item name="11. BEACON" action="OFF" /><item name="12. NOSE STEER" action="OFF" /><item name="13. PASSENGER SIGNS" action="OFF" /><item name="14. CABIN DIFF PRESSURE" action="0 PSI" /></checklist><checklist name="securing checklist SF"><item name=" 1. EMER LTS" action="OFF" /><item name=" 2. AC/DC UTILITY" action="OFF / 3 MINS" /><item name=" 3. THRUST REVERSERS" action="OFF" /><item name=" 4. CHOCKS" action="INSTALL" /><item name=" 5. PARKING BRAKE" action="OFF" /><item name=" 6. HYD PUMPS" action="OFF" /><item name=" 7. NAV &amp; LOGO LIGHTS" action="OFF" /><item name=" 8. APU GEN" action="OFF" /><item name=" 9. APU START/STOP" action="STOP" /><item name="1